program sudoku2;

var
	sdk:array [0..8,0..8] of integer;
	sdkAux:array [0..8,0..8] of integer;
	
	

	
function repetido (num,c1,c2:integer):boolean;

var
	x,y:integer;

begin
	repetido:=false;
	for x:=0 to 8 do
	begin
		if sdk[c1,x] = num then
		begin
			repetido:=true;
		end;
	end;
	
	for x:=0 to 8 do
	begin
		if sdk[x,c2] = num then
		begin
			repetido:=true;
		end;
	end;	
	
	for x:=0 to 2 do
	begin
		for y:=0 to 2 do
		begin
			if sdk[(c1 div 3 * 3)+x, (c2 div 3 * 3)+y] = num then
			begin
				repetido:=true;
			end;
		end;
	end;
end;


procedure resolverAux;

var	
	k,n,x,y,x0,y0:integer;
	
	
begin
	randomize;
	for x:=0 to 8 do
	begin
		for y:=0 to 8 do
		begin
			if sdkAux[x,y] = 0 then	
			begin
				for k:=1 to 9 do
				begin
					n:=random(8)+1;
					if not repetido(n,x,y) then
					begin
						sdk[x,y]:=n;
						resolver;
					end
					else
					begin
						resolverAux;
					end;
				end;
			end;
		end;
	end;
end;




procedure resolver;

var	
	k,n,x,y,x0,y0:integer;
	
	
begin
	randomize;
	for x:=0 to 8 do
	begin
		for y:=0 to 8 do
		begin
			if sdk[x,y] = 0 then
			begin
				for k:=1 to 9 do
				begin
					n:=random(8)+1;
					if not repetido(n,x,y) then
					begin
						sdk[x,y]:=n;
						resolver;
					end
					else
					begin
						resolverAux;
					end;
				end;
			end;
		end;
	end;
end;





BEGIN
	

	

END.
